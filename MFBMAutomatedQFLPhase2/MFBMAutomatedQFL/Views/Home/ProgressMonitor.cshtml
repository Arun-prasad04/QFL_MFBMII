            width: 15px !important;
        }

    .mCSB_1_scrollbar {
        margin: 12px 0px 12px 0 !important;
    }

        .mCSB_1_scrollbar > .mCSB_draggerContainer > .mCSB_dragger > .mCSB_dragger_bar {
            width: 4px !important;
        }







